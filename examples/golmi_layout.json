{
  "title": "Pentomino in GOLMI",
  "subtitle": "Multiplayer Pentomino game built using the GOLMI framework",
  "html": [
    {
      "layout-type": "div",
      "id": "viewport",
      "style": "height: 500px; width: 500px; padding: 10px;",
      "layout-content": [
        {
          "layout-type": "canvas",
          "id": "background",
          "width": "500",
          "height": "500"
        },
        {
          "layout-type": "canvas",
          "id": "objects",
          "width": "500",
          "height": "500"
        },
        {
          "layout-type": "canvas",
          "id": "gripper",
          "width": "500",
          "height": "500"
        }
      ]
    }
    ],
  "css": {
    "#viewport": {
      "position": "relative;"
    },
    "#viewport canvas": {
      "position": "absolute;"
    },
    "canvas": {
      "background-color": "transparent;",
      "border": "1px solid #000000;"
    }
  },
  "scripts": {
    "incoming-text": "display-text",
    "incoming-image": "display-image",
    "submit-message": "send-message",
    "print-history": "plain-history",
    "plain": [
      "golmi-js/view/View",
      "golmi-js/view/LayerView",
      "golmi-js/view/LView",
      "golmi-js/controller/LocalKeyController",
      "golmi-js/pentomino_game"
    ]
  }
}